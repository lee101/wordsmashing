{
    "name": "Word Smashing",
    "description": "Word search puzzle with a twist",
    "version": "3",
    "manifest_version": 2,
    "app": {
        "urls": [
            "*://www.wordsmashing.com/"
        ],
        "launch": {
            "web_url": "http://www.wordsmashing.com/"
        }
    },
    "icons": {
        "128": "icon_128.png"
    },
    "offline_enabled": "true",
    "permissions": [
        "unlimitedStorage",
        "notifications"
    ]
}